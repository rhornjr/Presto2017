<?xml version="1.0"?>
<configuration>

  <appSettings>    
    <add key="databaseNamespace"  value="PrestoCommon.Data.RavenDb"/>  <!-- RavenDB -->
    <add key="timerInterval"      value="20000"/>
  </appSettings>

  <connectionStrings>
    <add name="RavenDb" connectionString="Url=http://localhost:8080"/>
  </connectionStrings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>
